# Twilio Configuration
# Get these from your Twilio Console: https://console.twilio.com/
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_FROM_NUMBER=+1234567890

# ElevenLabs Configuration
# Get your Agent ID from ElevenLabs Dashboard
# This is the AI agent that will handle incoming calls
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id_here

# Google Calendar Configuration
# Calendar ID (usually your email or a specific calendar ID)
GOOGLE_CALENDAR_ID=your_email@gmail.com

# Google Service Account JSON (entire JSON as a single-line string)
# Create a service account in Google Cloud Console and download the JSON key
GOOGLE_SERVICE_ACCOUNT_JSON='{"type":"service_account","project_id":"..."}'

# Timezone for calendar events (IANA timezone format)
TIMEZONE=America/Los_Angeles
